<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  主题色  -->
    <SolidColorBrush x:Key="ThemeColor">#5B95FF</SolidColorBrush>
    <!--  蓝色  -->
    <SolidColorBrush x:Key="BlueColor">#326CF3</SolidColorBrush>

    <!--  绿色  -->
    <SolidColorBrush x:Key="GreenColor">#26DDA4</SolidColorBrush>

    <!--  黄色  -->
    <SolidColorBrush x:Key="YellowColor">#FFCB5B</SolidColorBrush>

    <!--  红色  -->
    <SolidColorBrush x:Key="RedColor">#DB3340</SolidColorBrush>

    <!--  灰色1  -->
    <SolidColorBrush x:Key="GrayColor">#B3B2B7</SolidColorBrush>
    <SolidColorBrush x:Key="GrayColor1">#CCCCCC</SolidColorBrush>

    <!--  全白 透明色  -->
    <SolidColorBrush x:Key="TransparentColor" Color="#00FFFFFF" />
    <!--  全黑 透明色  -->
    <SolidColorBrush x:Key="TransparentBlackColor" Color="#00000000" />
    <!--  控件被禁用的背景颜色  -->
    <SolidColorBrush x:Key="ListBox.Disabled.Background" Color="#FFFFFFFF" />
    <!--  控件被禁用的边框颜色  -->
    <SolidColorBrush x:Key="ListBox.Disabled.Border" Color="#FFD9D9D9" />


    <SolidColorBrush x:Key="Foreground" Color="Black" />
    <SolidColorBrush x:Key="IsMouseOver.Background" Color="#D7DDE4" />
    <SolidColorBrush x:Key="IsMouseOver.BorderBrush" Color="#448EC9" />
    <SolidColorBrush x:Key="IsMouseOver.Foreground" Color="White" />

    <SolidColorBrush x:Key="IsSelected.Background" Color="#448EC9" />
    <SolidColorBrush x:Key="IsSelected.BorderBrush" Color="#448EC9" />
    <SolidColorBrush x:Key="IsSelected.Foreground" Color="White" />

    <!--  TabControl相关  -->
    <SolidColorBrush x:Key="TabPanel.Background" Color="#D7DDE4" />
    <SolidColorBrush x:Key="TabPanel.BorderBrush" Color="#AFB7C1" />
    <SolidColorBrush x:Key="TabItem.Background" Color="#D7DDE4" />
    <SolidColorBrush x:Key="TabItem.BorderBrush" Color="#AFB7C1" />
    <SolidColorBrush x:Key="TabItem.IsMouseOver.BorderBrush" Color="#22599D" />
    <SolidColorBrush x:Key="TabItem.IsMouseOver.Background" Color="#CADDE9" />
    <SolidColorBrush x:Key="TabItem.IsSelected.BorderBrush" Color="#B1C7E1" />
    <SolidColorBrush x:Key="TabItem.IsSelected.Background" Color="#729BBC" />

    <!--  GroupBox相关  -->
    <SolidColorBrush x:Key="GroupBox.Header.Foreground" Color="#5B81A0" />
    <SolidColorBrush x:Key="GroupBox.Header.Background" Color="#EDEDF3" />
    <SolidColorBrush x:Key="GroupBox.Header.BorderBrush" Color="#B0B9C4" />
    <SolidColorBrush x:Key="GroupBox.Background" Color="#F0F3F6" />

</ResourceDictionary>