<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  主题色  -->
    <SolidColorBrush x:Key="ThemeColor">#5B95FF</SolidColorBrush>

    <!--  蓝色  -->
    <SolidColorBrush x:Key="BlueColor">#326CF3</SolidColorBrush>

    <!--  绿色  -->
    <SolidColorBrush x:Key="GreenColor">#26DDA4</SolidColorBrush>

    <!--  黄色  -->
    <SolidColorBrush x:Key="YellowColor">#FFCB5B</SolidColorBrush>

    <!-- 红色  -->
    <SolidColorBrush x:Key="RedColor">#DB3340</SolidColorBrush>

    <!--  灰色1  -->
    <SolidColorBrush x:Key="GrayColor">#B3B2B7</SolidColorBrush>
    <SolidColorBrush x:Key="GrayColor1">#CCCCCC</SolidColorBrush>

    <!--  全白 透明色  -->
    <SolidColorBrush x:Key="TransparentColor" Color="#00FFFFFF" />
    <!--  全黑 透明色  -->
    <SolidColorBrush x:Key="TransparentBlackColor" Color="#00000000" />
</ResourceDictionary>